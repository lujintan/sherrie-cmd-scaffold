{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "/",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "index": {
            "id": "index",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "tpl": {
                    "id": "tpl",
                    "type": "string",
                    "minLength": 1,
                    "enum": [
                        "skiing",
                        "hotSpring",
                        "landscape",
                        "oldtown",
                        "playground",
                        "archeology"
                    ]
                },
                "title": {
                    "id": "title",
                    "type": "string",
                    "minLength": 1
                },
                "style": {
                    "id": "style",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "themeColor": {
                            "id": "themeColor",
                            "type": "string",
                            "minLength": 1
                        },
                        "layout": {
                            "id": "layout",
                            "type": "string",
                            "minLength": 1,
                            "enum": [
                                "card",
                                "fullcolumn"
                            ]
                        },
                        "backgroundImg": {
                            "id": "backgroundImg",
                            "type": "string",
                            "minLength": 1,
                            "default": null
                        }
                    },
                    "required": ["themeColor", "layout"]
                },
                "navs": {
                    "id": "navs",
                    "type": "array",
                    "minItems": 1,
                    "uniqueItems": false,
                    "additionalItems": false,
                    "items": {
                        "id": "3",
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "text": {
                                "id": "text",
                                "type": "string",
                                "minLength": 1
                            },
                            "name": {
                                "id": "name",
                                "type": "string",
                                "minLength": 1,
                                "enum": [
                                    "ordercenter",
                                    "tickets",
                                    "spotlist",
                                    "introduce",
                                    "routes",
                                    "gallery",
                                    "heatmap",
                                    "weather",
                                    "tourguide"
                                ]
                            },
                            "action": {
                                "id": "action",
                                "type": "object",
                                "additionalProperties": false,
                                "properties": {
                                    "type": {
                                        "id": "type",
                                        "type": "string",
                                        "minLength": 1,
                                        "enum": [
                                            "link",
                                            "module"
                                        ]
                                    },
                                    "value": {
                                        "id": "value",
                                        "type": "string",
                                        "minLength": 1
                                    }
                                },
                                "required": ["type","value"]
                            }
                        },
                        "required": ["text", "name", "action"]
                    }
                }
            },
            "required": [
                "tpl",
                "title",
                "style",
                "navs"
            ]
        },
        "modules": {
            "id": "modules",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "introduce": {
                    "id": "introduce",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "name": {
                            "id": "name",
                            "type": "string",
                            "minLength": 1
                        },
                        "address": {
                            "id": "address",
                            "type": "string",
                            "minLength": 1
                        },
                        "traffic": {
                            "id": "traffic",
                            "type": "string",
                            "minLength": 1
                        },
                        "open": {
                            "id": "open",
                            "type": "string",
                            "minLength": 1
                        },
                        "best": {
                            "id": "best",
                            "type": "string",
                            "minLength": 1
                        },
                        "detail": {
                            "id": "detail",
                            "type": "string",
                            "minLength": 1
                        },
                        "image": {
                            "id": "image",
                            "type": "string",
                            "minLength": 1
                        }
                    }
                },
                "gallery": {
                    "id": "gallery",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "from": {
                            "id": "from",
                            "type": "string",
                            "minLength": 1,
                            "enum": [
                                "baidulvyou",
                                "customer"
                            ]
                        },
                        "source": {
                            "id": "source",
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "customer": {
                                    "id": "customer",
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "items": {
                                            "id": "items",
                                            "type": "array",
                                            "minItems": 1,
                                            "uniqueItems": false,
                                            "additionalItems": false,
                                            "items": {
                                                "id": "1",
                                                "type": "object",
                                                "additionalProperties": false,
                                                "properties": {
                                                    "images": {
                                                        "id": "images",
                                                        "type": "array",
                                                        "minItems": 1,
                                                        "uniqueItems": false,
                                                        "additionalItems": false,
                                                        "items": {
                                                            "id": "2",
                                                            "type": "object",
                                                            "additionalProperties": false,
                                                            "properties": {
                                                                "title": {
                                                                    "id": "title",
                                                                    "type": "string",
                                                                    "minLength": 0
                                                                },
                                                                "url": {
                                                                    "id": "url",
                                                                    "type": "string",
                                                                    "minLength": 1
                                                                }
                                                            }
                                                        }
                                                    },
                                                    "name": {
                                                        "id": "name",
                                                        "type": "string",
                                                        "minLength": 1
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "tourguide": {
                    "id": "tourguide",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "image": {
                            "id": "image",
                            "type": "string",
                            "minLength": 1
                        }
                    }
                },
                "routes": {
                    "id": "routes",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "outerTraffic": {
                            "id": "outerTraffic",
                            "type": "string",
                            "minLength": 1
                        },
                        "innerTraffic": {
                            "id": "innerTraffic",
                            "type": "string",
                            "minLength": 1
                        },
                        "tourAddress": {
                            "id": "tourAddress",
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "address": {
                                    "id": "address",
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "province": {
                                            "id": "province",
                                            "type": "string",
                                            "minLength": 1
                                        },
                                        "city": {
                                            "id": "city",
                                            "type": "string",
                                            "minLength": 1
                                        },
                                        "regional": {
                                            "id": "regional",
                                            "type": "string",
                                            "minLength": 1
                                        },
                                        "street": {
                                            "id": "street",
                                            "type": "string",
                                            "minLength": 1
                                        }
                                    }
                                },
                                "point": {
                                    "id": "point",
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "lat": {
                                            "id": "lat",
                                            "type": "number",
                                            "maximum": 90,
                                            "minimum": -90,
                                            "exclusiveMaximum": false,
                                            "exclusiveMinimum": false
                                        },
                                        "lng": {
                                            "id": "lng",
                                            "type": "number",
                                            "maximum": 180,
                                            "minimum": -180,
                                            "exclusiveMaximum": false,
                                            "exclusiveMinimum": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "required": [
                        "outerTraffic",
                        "innerTraffic",
                        "tourAddress"
                    ]
                },
                "spotlist": {
                    "id": "spotlist",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "from": {
                            "id": "from",
                            "type": "string",
                            "minLength": 1
                        },
                        "source": {
                            "id": "source",
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "customer": {
                                    "id": "customer",
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "items": {
                                            "id": "items",
                                            "type": "array",
                                            "minItems": 1,
                                            "uniqueItems": false,
                                            "additionalItems": false,
                                            "items": {
                                                "id": "2",
                                                "type": "object",
                                                "additionalProperties": false,
                                                "properties": {
                                                    "name": {
                                                        "id": "name",
                                                        "type": "string",
                                                        "minLength": 1
                                                    },
                                                    "image": {
                                                        "id": "image",
                                                        "type": "string",
                                                        "minLength": 1
                                                    },
                                                    "brief": {
                                                        "id": "brief",
                                                        "type": "string",
                                                        "minLength": 1
                                                    },
                                                    "introduce": {
                                                        "id": "introduce",
                                                        "type": "string",
                                                        "minLength": 1
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "required": [
                        "from",
                        "source"
                    ]
                },
                "heatmap": {
                    "id": "heatmap",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "province": {
                            "id": "province",
                            "type": "string",
                            "minLength": 1
                        },
                        "city": {
                            "id": "city",
                            "type": "string",
                            "minLength": 1
                        },
                        "poiname": {
                            "id": "poiname",
                            "type": "string",
                            "minLength": 1
                        },
                        "centerPoint": {
                            "id": "centerPoint",
                            "type": "string",
                            "minLength": 1
                        },
                        "x": {
                            "id": "x",
                            "type": "number",
                            "maximum": 360,
                            "minimum": -360,
                            "exclusiveMaximum": false,
                            "exclusiveMinimum": false
                        },
                        "y": {
                            "id": "y",
                            "type": "number",
                            "maximum": 360,
                            "minimum": -360,
                            "exclusiveMaximum": false,
                            "exclusiveMinimum": false
                        }
                    }
                },
                "weather": {
                    "id": "weather",
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "province": {
                            "id": "province",
                            "type": "string",
                            "minLength": 1
                        },
                        "city": {
                            "id": "city",
                            "type": "string",
                            "minLength": 1
                        }
                    }
                }
            }
        }
    },
    "required": [
        "index",
        "modules"
    ]
}